{
  "name": "Messagram",
  "version": "1.0.0",
  "description": "Messagram Telegram client App",
  "launch_path": "/index.html",
  "type": "certified",
  "icons": {
    "56": "/icon56.png",
    "112": "/icon112.png"
  },
  "developer": {
    "name": "Ikhlas Risandy",
    "url": "https://github.com/kucira"
  },
  "locale": {
    "en-US":{
      "name": "Messagram",
      "description": "Messagram App"
    }
  },
  "default_locale": "en-US",
  "subtitle": "Messagram",
  "origin": "app://messagram.herokuapp.com",
  "permissions":{
    "input":{
      "description": "Required to enter values"
    },
    "push": {},
    "desktop-notification": {},
    "systemXHR": {},
    "alarms": {},
    "themeable": {},
    "browser": {},
    "audio-channel-notification": {},
    "contacts": {
      "access": "readcreate"
    },
    "settings": {
      "access": "readwrite"
    },
    "downloads": {},
    "device-storage:sdcard": {
      "access": "readcreate"
    },
    "tcp-socket": {},
    "softkey": {},
    "mobileconnection": {},
    "engmode-extension": {},
    "flip": {},
    "power": {}
  },
  "activities": {
    "open-fb": {
      "href": "./index.html"
    },
    "share": {
      "href": "/index.html",
      "filters": {
        "type": ["image/*"]
      },
      "disposition": "window",
      "returnValue": true
    }
  },
  "orientation": [ "portrait-primary" ],
  "messages": [
    { "push": "/index.html" },
    { "push-register": "/index.html" },
    { "serviceworker-notification": "/index.html" }
  ]
}